{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/checkout-sessions.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/create-checkout-session.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/generate-image.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/webhooks.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/checkout-sessions",
      "dest": "/api/checkout-sessions.js"
    },
    {
      "src": "/api/create-checkout-session",
      "dest": "/api/create-checkout-session.js"
    },
    {
      "src": "/api/generate-image",
      "dest": "/api/generate-image.js"
    },
    {
      "src": "/api/webhooks",
      "dest": "/api/webhooks.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/photos/(.*)",
      "dest": "/photos/$1"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico))",
      "dest": "/$1"
    },
    {
      "src": "/success",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}